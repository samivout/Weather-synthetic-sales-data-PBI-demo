table RemoveWeatherDuplicates
	lineageTag: 0658ac7b-a7f5-463f-8662-69987fd3e517

	calculationGroup
		precedence: 5

		calculationItem CollapseWeatherDuplicates = ```
				
				VAR GroupedTable =
				    SUMMARIZE(
				        'F_Sales',
				        'F_Sales'[location_id],
				        'F_Sales'[Date],
				        'F_Sales'[hour],
				        "WeatherValue", SELECTEDMEASURE()
				    )
				RETURN
				    CALCULATE(
				        SELECTEDMEASURE(),
				        KEEPFILTERS(GroupedTable)
				    )
				
				```

